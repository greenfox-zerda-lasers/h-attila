{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "modules": false,
    "rules": {
      "no-var": "off",
      "no-console": "off",
      "strict": "off",
      "vars-on-top": "off",
      "func-names": ["error", "never"],
      "prefer-arrow-callback": ["error", { "allowNamedFunctions": false }]
    }
}
